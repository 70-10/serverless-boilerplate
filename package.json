{
  "name": "serverless-boilerplate",
  "version": "0.1.0",
  "description": "Serverless Framework Boilerplate",
  "main": "handler.js",
  "scripts": {
    "deploy": "serverless deploy -v",
    "dev": "run-s dynamo:init offline",
    "offline": "NODE_ENV=LOCALDEV sls offline start",
    "dynamo:init": "sls dynamodb install",
    "dynamo:gui": "DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin",
    "clean": "rimraf .nyc_output coverage",
    "lint": "eslint .",
    "report": "nyc report --reporter=html",
    "test": "nyc mocha test",
    "test:coverage": "run-s clean lint test report",
    "precommit": "lint-staged",
    "prepush": "mocha test"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": "6.10"
  },
  "keywords": [
    "serverless"
  ],
  "author": "70_10",
  "license": "MIT",
  "devDependencies": {
    "aws-sdk": "^2.116.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "co-mocha": "1.2.0",
    "dynamodb-admin": "^1.14.0",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "intelli-espower-loader": "1.0.1",
    "lint-staged": "^4.1.3",
    "mocha": "^3.5.3",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.0",
    "rimraf": "^2.6.2",
    "serverless": "^1.22.0",
    "serverless-dynamodb-local": "^0.2.25",
    "serverless-offline": "^3.15.3",
    "serverless-webpack": "^3.0.0",
    "webpack": "^3.5.6"
  },
  "dependencies": {}
}

{
  "name": "serverless-boilerplate",
  "version": "0.1.0",
  "description": "Serverless Framework Boilerplate",
  "main": "handler.js",
  "scripts": {
    "deploy": "serverless deploy -v",
    "dev": "run-s dynamo offline",
    "offline": "sls offline start --stage LOCALDEV",
    "dynamo": "sls dynamodb install",
    "clean": "rimraf .nyc_output coverage",
    "lint": "prettier-eslint --write \"./@(lib|test|config)/**/*.js\"",
    "report": "nyc report --reporter=html",
    "test": "nyc mocha test",
    "test:coverage": "run-s clean lint test report",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "engines": {
    "node": "6.10"
  },
  "keywords": [
    "serverless"
  ],
  "author": "70_10",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "co": "^4.6.0",
    "express": "^4.16.3",
    "serverless-http": "^1.5.5"
  },
  "devDependencies": {
    "aws-sdk": "^2.222.1",
    "co-mocha": "^1.2.2",
    "config": "^1.30.0",
    "eslint": "^4.19.1",
    "eslint-plugin-node": "^6.0.1",
    "husky": "^0.14.3",
    "intelli-espower-loader": "1.0.1",
    "lint-staged": "^7.0.4",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "power-assert": "^1.5.0",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.2",
    "serverless": "^1.26.1",
    "serverless-dynamodb-local": "^0.2.28",
    "serverless-offline": "^3.20.1"
  }
}

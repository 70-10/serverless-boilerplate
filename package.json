{
  "name": "serverless-boilerplate",
  "version": "0.1.0",
  "description": "Serverless Framework Boilerplate",
  "keywords": [
    "serverless"
  ],
  "license": "MIT",
  "author": "70_10",
  "main": "handler.js",
  "scripts": {
    "clean": "rimraf .nyc_output coverage",
    "deploy": "serverless deploy -v",
    "dev": "run-s dynamo offline",
    "dynamo": "sls dynamodb install",
    "lint": "prettier-eslint --write \"./@(lib|test|config)/**/*.js\"",
    "offline": "sls offline start --stage LOCALDEV",
    "precommit": "lint-staged",
    "report": "nyc report --reporter=html",
    "test": "nyc mocha test",
    "test:coverage": "run-s clean lint test report"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.1.0",
    "express": "^4.17.1",
    "serverless-http": "^2.0.2"
  },
  "devDependencies": {
    "aws-sdk": "^2.468.0",
    "eslint": "^5.16.0",
    "eslint-plugin-node": "^9.1.0",
    "husky": "^2.3.0",
    "intelli-espower-loader": "1.0.1",
    "lint-staged": "^8.1.7",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "power-assert": "^1.6.1",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.3",
    "serverless": "^1.44.1",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.0.1"
  },
  "engines": {
    "node": "^8.10"
  }
}

{
  "name": "serverless-boilerplate",
  "version": "0.1.0",
  "description": "Serverless Framework Boilerplate",
  "main": "handler.js",
  "scripts": {
    "deploy": "serverless deploy -v",
    "dev": "run-s dynamo offline",
    "offline": "sls offline start --stage LOCALDEV",
    "dynamo": "sls dynamodb install",
    "clean": "rimraf .nyc_output coverage",
    "lint": "prettier-eslint --write \"./@(lib|test|config)/**/*.js\"",
    "report": "nyc report --reporter=html",
    "test": "nyc mocha test",
    "test:coverage": "run-s clean lint test report",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "engines": {
    "node": "^8.10"
  },
  "keywords": [
    "serverless"
  ],
  "author": "70_10",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^3.0.0",
    "express": "^4.16.4",
    "serverless-http": "^1.8.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.368.0",
    "eslint": "^5.9.0",
    "eslint-plugin-node": "^8.0.0",
    "husky": "^1.2.0",
    "intelli-espower-loader": "1.0.1",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "power-assert": "^1.6.1",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.2",
    "serverless": "^1.34.0",
    "serverless-dynamodb-local": "^0.2.35",
    "serverless-offline": "^3.31.3"
  }
}

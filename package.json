{
  "name": "serverless-boilerplate",
  "version": "0.1.0",
  "description": "Serverless Framework Boilerplate",
  "main": "handler.js",
  "scripts": {
    "deploy": "serverless deploy -v",
    "dev": "run-s dynamo offline",
    "offline": "NODE_ENV=LOCALDEV sls offline start",
    "dynamo": "sls dynamodb install",
    "clean": "rimraf .nyc_output coverage",
    "lint": "eslint .",
    "report": "nyc report --reporter=html",
    "test": "nyc mocha test",
    "test:coverage": "run-s clean lint test report",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": "6.10"
  },
  "keywords": [
    "serverless"
  ],
  "author": "70_10",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "0.0.21",
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^8.0.53",
    "aws-sdk": "^2.156.0",
    "co-mocha": "^1.2.1",
    "config": "^1.28.1",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "intelli-espower-loader": "1.0.1",
    "lint-staged": "^5.0.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.2",
    "serverless": "^1.24.1",
    "serverless-dynamodb-local": "^0.2.26",
    "serverless-offline": "^3.16.0",
    "serverless-plugin-typescript": "^1.1.3"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "cheerio": "^1.0.0-rc.2",
    "co": "^4.6.0"
  }
}
